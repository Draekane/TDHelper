{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "block-scoped-var": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "indent": 0,
    "linebreak-style": 0,
    "new-cap": 0,
    "no-else-return": 0,
    "no-extend-native": 0,
    "no-irregular-whitespace": 0,
    "no-shadow": 0,
    "no-use-before-define": 0,
    "object-shorthand": 0,
    "padded-blocks": 0,
    "max-len": [ 2, 300, 4 ],
    "react/jsx-no-bind": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function":  0,
    "unicode-bom": 0
  },
  "plugins": [
    "react"
  ]
}
